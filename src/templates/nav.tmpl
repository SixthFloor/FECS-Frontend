<div class="nav-bar">
  <div class="left">
    <div class="logo"></div>
    <input class="navtf" id="searchtf" type="text" placeHolder="Search" />
    <button id="search" type="submit">Search</button>
  </div>
  <div class="right">
    <a href="#/register"><button class="navbutton" id="regbutton">Register</button></a>
    <button class="navbutton" id="loginbutton">Log in</button>

    <div class="login" ng-controller="LoginController as loginCtrl">
      <form role="form">
        <input class="navtf" id="usernametf" type="text" placeHolder="E-mail" ng-model="loginCtrl.email" />
        <input class="navtf" id="passwordtf" type="password" placeHolder="Password" ng-model="loginCtrl.pwd" /><br/>
        <input id="remember" type="checkbox" /><label for="remember"><span></span>Keep me logged in</label> |
        <a href="">Forgot your password?</a>
        <button id="loggingin" type="submit" ng-click="loginCtrl.login()">Log in</button>
      </form>
      Your email is {{loginCtrl.email}}
      with password {{loginCtrl.pwd}}
    </div>

  </div>
  <div class="clear"></div>
</div>
